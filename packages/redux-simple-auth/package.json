    "rimraf": "^2.6.2",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "invariant": "^2.2.3",
    "js-cookie": "^2.2.0",
    "lodash.isplainobject": "^4.0.6",
    "warning": "^3.0.0"
